{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"THIRD_PARTY_MODULES",
		"/hooks/",
		"/components/",
		"../(.*)",
		"./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
